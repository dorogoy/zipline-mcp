{
  "name": "zipline-mcp",
  "version": "1.1.0",
  "description": "An MCP server to upload files to a Zipline-compatible host.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "zipline-mcp": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "mkdir -p dist",
    "build": "tsc --outDir dist",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run format:check && npm run test:run && npm run build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "zipline",
    "file-upload",
    "server"
  ],
  "author": "Sergio Rael <npm.8ydjc@etereo.one>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dorogoy/zipline-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/dorogoy/zipline-mcp/issues"
  },
  "homepage": "https://github.com/dorogoy/zipline-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "typescript-eslint": "^8.39.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/node": "^20.19.10",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
